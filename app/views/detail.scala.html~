	//点击收藏
        $('.collect-span').toggle(function(){
		collect('add');
        },function(){
		collect('delete');
        });
	function collect(status){
		  $.post('/collect/addService',{'id':'55d7a85518827','status':status},function(result){
			if(result.code==1000){
				if(status=='add'){
					 $('.collect-img').addClass('current');
					$().popup('收藏成功');	
				}else if(status=='delete'){
					  $('.collect-img').removeClass('current');
					$().popup('取消收藏成功');	
				}	
			}else if(result.code==1007){
				$('.login-modal').modal('show');
				resterLogin(true);
			}else{
				$().popup(result.error);
			}
		},'json');
	}
        //电脑端左侧向导标签
        function guidTag(){
            //左侧向导标签添加
            var html = '<span class="tag-more"></span><div class="tag-hidden">';
            if($('.guide-tag1 span').length>=3){
                var num=0;
                $('.guide-tag1 span').each(function(){
                    if(num>=2){
                        html +='<span><i></i>'+$(this).text()+'</span>';
                        $(this).remove();
                    }
                    num++;
                });
                html +='<em></em></div>';
                $('.guide-tag1').append(html);
            }
            if($('.tag-hidden span').length==1){
                $('.tag-hidden').css({'width':146,'right':-166});
            }
            //左侧向导标签鼠标移入移出
            $('.tag-more').hover(function(){
                $(this).siblings('.tag-hidden').show();
            },function(){
                $(this).siblings('.tag-hidden').hide();
            })
        }
        //电脑端左侧咨询预定
        function order(){
            $('.order-btn').hover(function(){
                clearInterval(timer1);
                $('.order-main').show();
            },function(){
                timer1 = setInterval(function(){
                    $('.order-main').hide();
                }, 200);
            });
            $('.order-main .arrow').on('click',function(){
                $(this).parent('.order-main').hide();
            })
        }
        //左侧固定
        function fixedLeft(){
            var scrollTop=$(document).scrollTop();
            if(scrollTop<30){
                $(".guide-l").css("position","static");
                $('.guide-slide').css('padding-top',0);
                $('.guide-r').css({'padding-left':'30px','width':'728px'});
            }
            else if(($(document).outerHeight()-$(document).scrollTop()-$('.footer-section').outerHeight()-$('.navbar-fixed-top').outerHeight())<=$('.guide-l').outerHeight())
            {
                $('.guide-l').css({'position':'absolute','top':$(document).outerHeight()-$('.footer-section').outerHeight()-$('.guide-l').outerHeight()});
            }
            else
            {
                $(".guide-l").css({"position":"fixed","top":62,'z-index':'1029'});
                $('.guide-slide').css('padding-top',60);
                $('.guide-r').css({'padding-left':'272px','width':'970px'});
            }
        }
        //右侧视频
        function guideVideo(){
            $('.video-box span').on('click',function(){
                $('.video_main').get(0).play();
                $(this).hide();
                $(this).siblings('img').hide();
            });
            $('.video_main').on('ended',function(){
                $('.video-box span').show();
                $(this).siblings('img').show();
            });
            $('.video_main').on('play',function(){
                $('.video-box span').hide();
                $(this).children('img').hide();
            });
        }
        //右侧分享
        function guideShare(){
            $('.tag-share').hover(function(){
                clearInterval(timer3);
                $('.share-mian').show();
            },function(){
                timer3 = setInterval(function(){
                    $('.share-mian').hide();
                }, 200);
            });
            $('.share-mian .detel').on('click',function(){
                $('.share-mian').hide();
            })
        }
        //右侧向导资料、服务及费用、评论滚动固定
        function fixedRight(str){
            var scrollTop = $(document).scrollTop();
            var gao = $('.guide-photo').outerHeight()+30;//电脑端固定距离
            var mobileGao = $('.guide-photo').outerHeight()+2*$('.main-line').outerHeight()+$('.guide-message').outerHeight();//手机端固定距离
            var top2 = $('.guide-photo').outerHeight()+$('.slide-data').outerHeight()+$('.main-line').outerHeight();
            var top3 = top2 + $('.server-section').outerHeight()+$('.main-line').outerHeight();
            var mobiletop1 = $('.guide-photo').outerHeight()+$('.guide-message').outerHeight();
            var mobiletop2 = mobiletop1 + $('.slide-data').outerHeight()+20;
            var mobiletop3 = mobiletop2 + $('.server-section').outerHeight()+20;
            if(str==true){
                if(scrollTop<gao){
                    $('.guide-slide .title').css({'position':'relative','top':0});
                    $('.guide-slide').css('padding-top',0);
                }
                else{
                    if(scrollTop>=top3){
                        $('.guide-slide ul.title li').eq(2).addClass('active').siblings('li').removeClass('active');
                    }
                    else if(scrollTop>=top2){
                        $('.guide-slide ul.title li').eq(1).addClass('active').siblings('li').removeClass('active');
                    }
                    else if(scrollTop>=gao){
                        $('.guide-slide ul.title li').eq(0).addClass('active').siblings('li').removeClass('active');
                    }
                    $('.guide-slide .title').css({'position':'fixed','top':62});
                    $('.guide-slide').css('padding-top',60);
                }
            }
            else{
                if(scrollTop<mobileGao){
                    $('.guide-slide .title').css({'position':'relative','top':0});
                    $('.guide-slide').css('padding-top',0);
                }
                else{
                    if(scrollTop>=mobiletop3){
                        $('.guide-slide ul.title li').eq(2).addClass('active').siblings('li').removeClass('active');
                    }
                    else if(scrollTop>=mobiletop2){
                        $('.guide-slide ul.title li').eq(1).addClass('active').siblings('li').removeClass('active');
                    }
                    else if(scrollTop>=mobiletop1){
                        $('.guide-slide ul.title li').eq(0).addClass('active').siblings('li').removeClass('active');
                    }
                    $('.guide-slide .title').css({'position':'fixed','top':'1.76rem'});
                    $('.guide-slide').css('padding-top','1.5rem');
                }
            }
        }
        //右侧向导资料、服务及费用、评论点击切换效果
        function clickRight(str){
            var scrollTop = $(document).scrollTop();
            var top1 = $('.guide-photo').outerHeight();//电脑端固定距离
            var top2 = $('.guide-photo').outerHeight()+$('.slide-data').outerHeight()+$('.main-line').outerHeight();
            var top3 = top2 + $('.server-section').outerHeight()+$('.main-line').outerHeight();
            var mobiletop1 = $('.guide-photo').outerHeight()+$('.guide-message').outerHeight();
            var mobiletop2 = mobiletop1 + $('.slide-data').outerHeight()+20;
            var mobiletop3 = mobiletop2 + $('.server-section').outerHeight()+20;
            $('.guide-slide ul.title li').on('click',function(){
                $(this).addClass('active').siblings().removeClass('active');
                if(str==true){
                    if($(this).index()==0){
                        $("html,body").stop().animate({scrollTop: top1}, 500);
                    }
                    else if($(this).index()==1){
                        $("html,body").stop().animate({scrollTop: top2}, 500);
                    }
                    else if($(this).index()==2){
                        $("html,body").stop().animate({scrollTop: top3}, 500);
                    }
                }
                else{
                    if($(this).index()==0){
                        $("html,body").stop().animate({scrollTop: mobiletop1}, 500);
                    }
                    else if($(this).index()==1){
                        $("html,body").stop().animate({scrollTop: mobiletop2}, 500);
                    }
                    else if($(this).index()==2){
                        $("html,body").stop().animate({scrollTop: mobiletop3}, 500);
                    }
                }
            });
        }
        //评论图片判断
        function commentPhoto(){
            $('.comment-photo').each(function(){
                if($(this).find('li').size()>=5){
                    $(this).find('li').eq(4).css('margin-right',0);
                }
            })
        }
        //电脑端费用说明切换效果
        function serviceSlide(){
            $('.guide-service li').on('click',function(){
                $(this).addClass('active').siblings().removeClass('active');
                $('.guide-service .service-main').eq($(this).index()).show().siblings().hide();
            });
            $('.guide-plane li').on('click',function(){
                $(this).addClass('active').siblings().removeClass('active');
                $('.guide-plane .service-main').eq($(this).index()).show().siblings().hide();
            });
        }
        //更换相册模态框大图预览
        function photoChange(){
	var img=$('.lit-photo').find('.active').find('img').attr('src');
		img=img.split('@@');
		$('.big-img').attr('src',img[0]+'@@428h').one('load',function(){
			$('.big-img').css('margin-left',0);
			if($('.big-img').width()<640){
				var width=(640-$('.big-img').width())/2;
				$('.big-img').css('margin-left',width);
			}
		});
        }
        var key = 0;//当前小图片索引
        var n=8;
        // var total=$('.lit-photo li').length;
        //更换大图右侧按钮
        $('.next').on('click',function(){
            key++;
            if(key==total){
                if(getAlbum($(this).attr('data-id'),$(this).attr('data-name'))){
                    key=0;
                }else{
                    key--;
                    return false;
                }
                $('.lit-photo ul').stop().animate({'left':0},250);
            }else if(key==total-1){
                $('.big-photo span').text('下一相册：'+$(this).attr('data-name')).show();
                setTimeout(function(){
                    $('.big-photo span').hide();
                }, 1500);
            }else if(key>4){
                $('.lit-photo ul').stop().animate({'left':-$('.lit-photo li').width()*(key-4)},250);
            }
            $('.lit-photo li').eq(key).addClass('active').siblings().removeClass('active');
            photoChange();
            //n=key<5?8:(n+key-4);
            n=key<5?8:key+4;
            if(total-n>0){
                $('.right-arrow').show();
            }
            else{
                $('.right-arrow').hide();
            }
            $('.left-arrow').show();
            $('.guide-album h4 span').eq(0).text(key+1);
        });
        //鼠标移入右侧按钮且为相册最后一张
        $('.next').hover(function(){
            if(key==total-1){
                 $('.big-photo span').text('下一相册：'+$(this).attr('data-name')).show();
            }
        },function(){
            if(key==total-1){
                $('.big-photo span').hide();
            }
        });
        //更换大图左侧按钮
        $('.prev').on('click',function(){
            key--;
            if(key<0){
                if(getAlbum($(this).attr('data-id'),$(this).attr('data-name'))){
                    key=total-1;
                }else{
                    key++;
                    return false;
                }
                //$('.lit-photo ul').stop().animate({'left':-$('.lit-photo li').width()*(key-4)},250);
            }else if(key==0){
		$('.lit-photo li').eq(0).addClass('active').siblings().removeClass('active');
                $('.big-photo span').text('上一相册：'+$(this).attr('data-name')).show();
                setTimeout(function(){
                    $('.big-photo span').hide();
                }, 1500);		
		}else if(key==1){
			  $('.lit-photo ul').stop().animate({'left':0},250);s
		}else  if(key>4){
			    $('.lit-photo ul').stop().animate({'left':-$('.lit-photo li').width()*(key-4)},250);
		}
            $('.lit-photo li').eq(key).addClass('active').siblings().removeClass('active');
            photoChange();
            //n=key<5?8:(n+key-4);
            n=key<5?8:key+4;
            if(n-8>0){
                $('.left-arrow').show();
            }
            else{
                $('.left-arrow').hide();
            }
            $('.right-arrow').show();
            $('.guide-album h4 span').eq(0).text(key+1);
        });
        //鼠标移入左侧按钮且为相册第一张
        $('.prev').hover(function(){
            if(key==0){
                $('.big-photo span').text('上一相册：'+$(this).attr('data-name')).show();
            }
        },function(){
            if(key==0){
                $('.big-photo span').hide();
            }
        });
        //点击小图片
        $('.lit-photo li').live('click',function(){
            key=$(this).index();
            $(this).addClass('active').siblings().removeClass('active');
	$('.guide-album h4 span').eq(0).text(key+1);
            photoChange();
        })
        //右侧小箭头按钮
        $('.right-arrow').on('click',function(){
            if(total-n<=4){
                $(this).hide();
            }
            $('.left-arrow').show();
            n +=4;
            $('.lit-photo ul').stop().animate({'left':-$('.lit-photo li').width()*(n-8)},250);
        });
        //左侧小箭头按钮
        $('.left-arrow').on('click',function(){
            if(n-9<=4){
                $(this).hide();
            }
            $('.right-arrow').show();
            n -=4;
            $('.lit-photo ul').stop().animate({'left':-$('.lit-photo li').width()*(n-8)},250);
        });
        function getAlbum(id,name){
            if(!id){
                return false;
            }
            var prev_id=$('#'+id).prev().attr('id'),next_id=$('#'+id).next().attr('id');
            $('.prev').attr('data-id',(prev_id==undefined?'':prev_id));
            $('.prev').attr('data-name',(prev_id==undefined?'':$('#'+id).prev().attr('data-name')));
            $('.next').attr('data-id',(next_id==undefined?'':next_id));
            $('.next').attr('data-name',(next_id==undefined?'':$('#'+id).next().attr('data-name')));
	$().loading();
            $.getJSON('/album/getAlbum',{'id':id},function(result){
		$().unload();
                if(result.code==1000){
		total=0;
                    key=0;
		$('.lit-photo ul').stop().animate({'left':0},250);
                    var html='';
                    for(var i in result.result){
                        total++;
                        html +='<li '+(i==0?'class="active"':'')+' ><i></i><a href="javascript:;"><img src="'+result.result[i].img+'" alt=""></a></li>';
                    }
                    $('.guide-album h4').html(name+'<span>1</span>/<span>'+result.count+'</span>');
                    $('.guide-album ul').html(html)
                    $('.guide-album').modal('show');
                    // photoModal();
                    photoChange();
                }else{
		$().popup(result.error);
                }
            });
            return true;
        }
       //视频
        var parsentWidth = $('.video-box').width();
        var parsentHeight = parseInt(parsentWidth/1.6);
        var videoWidth = 0;
        var videoHeight = 0;
        $('.video-box img,.video-box').css('height',parsentHeight);
        if(isMobile()){
            $('.video_main').on('canplay',function(){
                videoWidth = $('.video_main').eq(0).width();
                videoHeight = $('.video_main').eq(0).height();
                if(videoWidth/videoHeight>1.6){
                    var _h = parseInt(videoHeight*$(window).width()/videoWidth);
                    $('.video_main').css({'width':$(window).width(),'height':_h,'margin-top':(parsentHeight-_h)/2});
                }
                else{
                    var _w = parseInt(videoWidth*$('.video-box').height()/videoHeight);
                    $('.video_main').css({'width':_w,'height':$('.video-box').height(),'margin-left':(parsentWidth-_w)/2});
                }
                console.log(videoWidth,videoHeight,_h)
            });
        }
        else{
            $('.video_main').on('canplay',function(){
                videoWidth = $('.video_main').eq(0).width();
                videoHeight = $('.video_main').eq(0).height();
                if(videoWidth/videoHeight>1.6){
                    var _h = parseInt(videoHeight*698/videoWidth);
                    $('.video_main').css({'width':698,'height':_h,'margin-top':(parsentHeight-_h)/2});
                }
                else{
                    var _w = parseInt(videoWidth*436/videoHeight);
                    $('.video_main').css({'width':_w,'height':436,'margin-left':(parsentWidth-_w)/2});
                }
            });
        }
    });
    </script>
